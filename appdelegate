#import "AppDelegate.h"
#import "MainViewController.h"
#import "SpotifyiOS.h"

@interface AppDelegate ()

@property(nonatomic, strong) SpotifyiOS *rootViewController;
@end
@implementation AppDelegate

- (BOOL)application:(UIApplication*)application didFinishLaunchingWithOptions:(NSDictionary*)launchOptions
{
    NSLog(@"this is application");
    self.window = [[UIWindow alloc] initWithFrame:UIScreen.mainScreen.bounds];
    self.viewController = [[MainViewController alloc] init];
    self.window.rootViewController = [SpotifyiOS new];
    self.rootViewController = [SpotifyiOS new];
    self.window.rootViewController = self.rootViewController;
    [self.window makeKeyAndVisible];
    return [super application:application didFinishLaunchingWithOptions:launchOptions];
}

- (BOOL)application:(UIApplication *)application openURL:(NSURL *)URL options:(NSDictionary<UIApplicationOpenURLOptionsKey, id> *)options
{
    [self.rootViewController.sessionManager application:application openURL:URL options:options];
    NSLog(@"jobs done %@", URL);
    return YES;
}

@end
