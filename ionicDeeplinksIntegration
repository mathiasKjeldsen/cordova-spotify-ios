NSString *URL = url.absoluteString;

    if ([URL rangeOfString:@"callback"].location == NSNotFound) {
      return [self application:app openURL:url sourceApplication:sourceApp annotation:annotation];
    } else {
      if([[SpotifyRemote sharedInstance]eventCallbackId] != nil) {
          [[SpotifyRemote sharedInstance] emit:@"CONNECT" withError:nil];
          [[SpotifyRemote sharedInstance] authParamsFromURL:url];
      }
      return [[SpotifyiOS sharedInstance] application:app openURL:url options:options];
        
    }